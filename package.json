{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "win64": "electron-packager . Bot_cp --out=dist/ --platform=win32 --arch=x64 --icon=icon.ico --overwrite",
    "linux": "electron-packager . Bot_cp --out=dist/ --platform=linux --arch=x64 --icon=icon.ico --overwrite"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "devDependencies": {
    "electron": "6.1.12"
  },
  "dependencies": {
    "electron-packager": "^17.1.1",
    "puppeteer": "13.1.3",
    "telegraf": "^4.11.2"
  }
}
